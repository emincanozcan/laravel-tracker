{
  "name": "emincan/laravel-tracker",
  "description": "Tracker library for the Laravel framework.",
  "type": "library",
  "authors": [
    {
      "name": "Emincan Ozcan",
      "email": "emincanozcann@gmail.com"
    }
  ],
  "require-dev": {
    "orchestra/testbench": "^6.12",
    "friendsofphp/php-cs-fixer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Emincan\\Tracker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Emincan\\Tracker\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix . --allow-risky=yes"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Emincan\\Tracker\\TrackerServiceProvider"
      ]
    }
  }
}
